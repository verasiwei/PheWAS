#' Install the required packages

packages <- c("PheWAS","Matrix","devtools","dplyr",
              "tidyr","ggplot2","parallel","MASS",
              "meta","ggrepel","DT","MatchIt","spam",
              "readxl")

install.packages(packages)

library(devtools)
library(PheWAS)
library(dplyr)
library(tidyr)
library(ggplot2)
library(parallel)
library(MASS)
library(meta)
library(ggrepel)
library(DT)
library(Matrix)
library(MatchIt)
library(spam)
library(readxl)
